type User {
  id: ID! @unique
  email: String! @unique
  name: String!
  username: String!
  photo: String
}

type Project {
  id: ID! @unique
  repo: @Repository
  healthcheck_threshold_seconds: Int
  healthcheck_url: String
  retry_deploy_limit: Int
}

type Repository {
  id: ID! @unique
  repo_id: Int!
  repo_name: String!
  repo_url: String!
  repo_type: String
}

type Deploy {
  id: ID! @unique,
  name: String!
}